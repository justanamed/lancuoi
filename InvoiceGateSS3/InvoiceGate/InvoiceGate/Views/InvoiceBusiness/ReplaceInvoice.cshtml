                                    <th class="d-lg-table-cell "> Xóa</th>
                                </tr>
                            </thead>
                            <tbody id="tbl_posts_body">
                                @{ foreach (var item in Model.InvoiceData.Items.Item.Select((value, i) => new { i, value }))
                                    {
                                        var temp = item.i + 1;
                                        <tr id="rec-@temp" class="package-row">
                                            <td><span class="sn">@temp</span>.</td>
                                            <td>Hàng hóa</td>
                                            <td contenteditable="true">@item.value.ItemCode</td>
                                            <td contenteditable="true">@item.value.ItemName</td>
                                            <td contenteditable="true">@item.value.UnitName</td>
                                            <td contenteditable="true">@item.value.BatchNo</td>
                                            <td contenteditable="true">@item.value.ExpDate</td>
                                            <td contenteditable="true" name="numberUsed" id="numberUsed" onkeyup="getValues()">@item.value.Quantity</td>
                                            <td contenteditable="true" id="price" onkeyup="getValues()">@item.value.UnitPrice</td>
                                            <td onchange="tinhtongtien()" class="thanhtien" name="revenue" id="revenue">@item.value.ItemTotalAmountWithoutVat</td>
                                            <td contenteditable="true" name="value" id="value">@item.value.ItemNote</td>
                                            <td><a class="btn btn-xs delete-record" data-id="@temp"><i class="fa fa-trash" aria-hidden="true"></i></a></td>
                                        </tr>
                                      }
                                    }
                                </tbody>
                        </table>
