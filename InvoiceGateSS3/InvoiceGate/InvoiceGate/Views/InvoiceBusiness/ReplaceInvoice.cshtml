                                        if ($('#tienthue').val() >= 0) {
                                            document.getElementById("tongtienthue").value = (tienthue * tientruocthue) / 100;
                                            document.getElementById("tongtienhang").value = (parseFloat(tientruocthue) + parseFloat($('#tongtienthue').val())) - parseFloat(chietkhau);
